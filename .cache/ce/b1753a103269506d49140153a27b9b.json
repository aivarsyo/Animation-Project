y: 'bahn';\r\n    src: url('Font/BAHNSCHRIFT.ttf')  format('truetype');\r\n  }\r\n\r\nbody{\r\n    background: #fff7e3;\r\n    margin: 0;\r\n    overflow: hidden;\r\n}\r\n\r\n.body-bck{\r\n    background: url(background2.svg);\r\n    background-size: cover;\r\n    background-repeat: repeat-x;\r\n    background-position: -60%;\r\n}\r\n\r\n#carParts, #CarParts {\r\n    height: 100vh;\r\n    width: 100vw;\r\n}\r\n\r\n#assemblyLine{\r\n    width: 0;\r\n    height: 0;\r\n}\r\n\r\n#infoMain{\r\n    width: 0;\r\n    height: 0;\r\n}\r\n\r\n#CarParts > g{\r\n    transform-origin: 0 0;\r\n}\r\n\r\n#FordLogo{\r\n    cursor: pointer;\r\n}\r\n\r\n#redButton{\r\n    cursor: pointer;\r\n}\r\n\r\n/* ---------------------------------------- */\r\n\r\n:root {\r\n    --bgPos: 0px;\r\n  }\r\n\r\n  .hidden{\r\n      display: none;\r\n  }\r\n  \r\n  #background {\r\n    height: 100vh;\r\n    width: 100vw;\r\n    overflow: hidden;\r\n  }\r\n  \r\n  #background svg {\r\n    height: 100%;\r\n    position: relative;\r\n    transform: translate(var(--bgPos));\r\n  }\r\n\r\n  #car, #pedalsSection{\r\n      position: absolute;\r\n      width: 40%;\r\n      height: auto;\r\n  }\r\n\r\n  #car{\r\n      top: 45vh;\r\n      left: 5vw;\r\n  }\r\n\r\n  #pedalsSection{\r\n      left: 58vw;\r\n      bottom: 5vh;\r\n      width: 20%;\r\n  }\r\n\r\n  #pedalsGroup > image{\r\n      cursor: pointer;\r\n  }\r\n\r\n  #checkpoints > image{\r\n    cursor: pointer;\r\n  }\r\n\r\n/* ------------------------------------ */\r\n\r\n// Ending\r\n\r\n#fuelAlert {\r\n    width: 30vw;\r\n    position: absolute;\r\n    bottom: 20vh;\r\n    left: 60vw;\r\n    cursor: pointer;\r\n  }\r\n  \r\n  #adviceBox {\r\n    position: absolute;\r\n    z-index: 100;\r\n    left: 25vw;\r\n    background-color: aquamarine;\r\n    width: 50vw;\r\n    height: 50vh;\r\n    text-align: center;\r\n    border: 1px solid black;\r\n    animation: introPopupKeys 1 0.4s ease;\r\n  }\r\n  \r\n  .fuelAnim {\r\n    animation: fuelAnimKeys 1s ease-out infinite;\r\n  }\r\n  \r\n  .smokeAnim {\r\n    position: absolute;\r\n    animation: smokeAnimKeys 1 0.5s ease;\r\n    width: 7vw;\r\n    bottom: 10vh;\r\n    right: 21vw;\r\n  }\r\n  \r\n  @keyframes fuelAnimKeys {\r\n    0% {\r\n      transform: scale(1);\r\n    }\r\n    90% {\r\n      transform: scale(0.9);\r\n    }\r\n    100% {\r\n      transform: scale(1);\r\n    }\r\n  }\r\n  \r\n  @keyframes introPopupKeys {\r\n    0% {\r\n      transform: scale(0);\r\n    }\r\n    90% {\r\n      transform: scale(1.05);\r\n    }\r\n    100% {\r\n      transform: scale(1);\r\n    }\r\n  }\r\n  \r\n  @keyframes smokeAnimKeys {\r\n    0% {\r\n      transform: scale(1);\r\n      transform: translate(-100px, 0px);\r\n    }\r\n    50% {\r\n      transform: scale(2);\r\n      transform: translate(-150px, 0px);\r\n    }\r\n    100% {\r\n      transform: scale(0.5);\r\n      transform: translate(-200px, 0px);\r\n    }\r\n  }\r\n\r\n/*   ----------------------- */\r\n\r\n#modalEngine{\r\n    position: absolute;\r\n    width: 100vw;\r\n    height: 100vh;\r\n    background-color: rgb(0,0,0); \r\n    background-color: rgba(0,0,0,0.9); \r\n    z-index: 200;\r\n}\r\n\r\n\r\n#modalEngine>svg{\r\n    text-align: center;\r\n    /* transform: scale(0.6); */\r\n    width: 100vw;\r\n    height: 100vh;\r\n}\r\n.close {\r\n  position: absolute;\r\n  top: 40px;\r\n  right: 40px;\r\n  color: #f1f1f1;\r\n  font-size: 80px;\r\n  font-weight: bold;\r\n  transition: 0.3s;\r\n  z-index: 201;\r\n}\r\n\r\n.close:hover,\r\n.close:focus {\r\n  color: #bbb;\r\n  text-decoration: none;\r\n  cursor: pointer;\r\n}\r\n"},"lineCount":null}},"error":null,"hash":"a52d591f2458c5a56baf946d210f85fc","cacheData":{"env":{}}}